$ErrorActionPreference = 'Stop'

$toolsPath = Split-Path $MyInvocation.MyCommand.Definition
$file = ''

$packageArgs = @{
  packageName    = ''
  fileType       = ''
  UnzipLocation	 = "$(Split-Path -Parent $MyInvocation.MyCommand.Definition)"
  file           = "$toolsPath\$file"
  validExitCodes = @(0)
  softwareName   = ''
}

Install-ChocolateyZipPackage @packageArgs
